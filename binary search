#include<iostream>
using namespace std;
int main()
{
    int a[5]={1,2,3,4,5};
    int key;
    cin>>key;
    int s=0;
    int e=4;
    int m=(s+e)/2;
    while(e>=s)
    {
        m=(s+e)/2;
        if(a[m]==key)
    {
        cout<<m;
        break;
    }
    else if(key>a[m])
    {
        s=m+1;
      
    }
    else{
        e=m-1;
    }
    }
//complte proess here
    return 0;
    
}
